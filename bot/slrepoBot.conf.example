[SLACK]
app_token = xapp-your-slack-app-token
bot_token = xoxb-your-slack-bot-token

[FILES]
csv_file_dir = ../data
csv_file_prefix = 구성관리조회
csv_file_extension = .csv
out_file_dir = ../output

[LOGGING]
log_file_dir = ../output
log_file = slrepoBot.log
log_level = INFO

[TEMPLATES]
info_template = 
- 서비스: {서비스}
- 역할: {IT구성정보명} {자산 설명}
- 유지보수: {유지보수 업체} {유지보수담당자}
- 담당자:
  . HW: {HW 소유부서} {HW 담당자}
  . SW: {SW 소유부서} {SW 관리자}
- 도입년월: {도입년월}

[COLUMNS]
info_columns = 서비스,IT구성정보명,자산 설명,유지보수 업체,유지보수담당자,HW 소유부서,HW 담당자,SW 소유부서,SW 관리자,도입년월

[QUEUE]
use_queue = false
redis_host = localhost
redis_port = 6379
redis_db = 0
timeout = 300